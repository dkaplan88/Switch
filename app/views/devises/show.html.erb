<%= high_chart("container", @h) %>

<h1>Action History</h1>
<%= link_to "Add an Action", new_history_path %>
<p>
  <% i = 1 %>
  <% @devise.histories.each do |x| %>
    <b>Action #<%= i %></b><br>
    Call:  <%= x.action %><br>
    Level:  <%= x.level %><br>
    <br>
    <% i += 1 %>
  <% end %>
</p>

