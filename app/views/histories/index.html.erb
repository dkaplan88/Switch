<h1>Listing histories</h1>

<table>
  <tr>
    <th>Devise</th>
    <th>Action</th>
    <th>Status</th>
    <th>Level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @histories.each do |history| %>
  <tr>
    <td><%= link_to history.devise.name, devise_path(history.devise_id) %></td>
    <td><%= history.action %></td>
    <td><%= history.status %></td>
    <td><%= history.level %></td>
    <td><%= link_to 'Show', history %></td>
    <td><%= link_to 'Edit', edit_history_path(history) %></td>
    <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New History', new_history_path %>
